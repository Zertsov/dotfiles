#! /bin/zsh

# General bash/zsh
alias ls="ls -FG"
alias s="source ~/.zshrc"

# Docker
alias dc="docker-compose"
alias dcu="docker-compose up"
alias dcd="docker-compose down"
alias dcb="docker-compose build"
alias dps="docker ps"
alias dimgs="docker images"

# Utility functions
alias pscan="portscan"